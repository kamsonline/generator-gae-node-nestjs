{
  "name": "iecl-scheduler-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/moment": "0.0.2",
    "@material-ui/core": "3.7.0",
    "@material-ui/icons": "3.0.1",
    "@types/graphql": "14.0.3",
    "@types/lodash": "^4.14.110",
    "@types/query-string": "^6.1.0",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "apollo-cache": "^1.1.12",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-codegen": "^0.20.2",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "classnames": "^2.2.6",
    "final-form": "^4.8.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "material-ui-pickers": "2.0.4",
    "moment": "2.23.0",
    "query-string": "^6.1.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-dom": "^16.4.1",
    "react-dropzone": "^4.2.13",
    "react-final-form": "^3.6.4",
    "react-progress-bar-plus": "^1.3.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "universal-cookie": "^2.2.0"
  },
  "scripts": {
    "format": "prettier --write \"./src/**/*.ts{,?}\"",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && mv ./build/ ../server/public",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "introspect": "apollo-codegen introspect-schema http://localhost:8080/api/graphql --output graphql.schema.json --header='x-csrf-token:development'",
    "generate": "apollo-codegen generate \"src/**/*.ts{,?}\" --tag-name gql --schema graphql.schema.json --target typescript --output src/graphql.ts",
    "graphql": "npm run introspect && npm run generate"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "prettier": "^1.13.7",
    "react-scripts-ts": "^2.16.0",
    "typescript": "^3.0.1"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080"
    },
    "/auth": {
      "target": "http://localhost:8080"
    },
    "/tasks": {
      "target": "http://localhost:8080"
    },
    "/system": {
      "target": "http://localhost:8080"
    }
  }
}
